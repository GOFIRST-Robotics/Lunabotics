        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>

    <include>
      <uri>
        https://fuel.gazebosim.org/1.0/OpenRobotics/models/Moon DEM
      </uri>
    </include>

    <include>
        <uri>
            https://fuel.gazebosim.org/1.0/OpenRobotics/models/Falling Rock 1
        </uri>
        <pose>0 0 15 0 0 0</pose>
