      </include> 

      <include>
        <uri>/src/gazebo/ros_gz_description/models/Apriltag36_11_00002</uri>
        <pose relative_to='bottom_left'>-6.87 1.2159 0.4 1.57 0 1.57</pose> <!-- TODO: Check where this should actually be mounted. -->
        <name>AprilTag_2</name>
