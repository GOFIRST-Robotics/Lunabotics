<?xml version="1.0" ?>
<sdf version='1.7'>
  <model name='COSMIC_field'>
    <static>true</static>
    <pose>0 0 0 0 0 0</pose>
      <link name='field'>
        <collision name="collision">
          <geometry>
            <mesh>
              <uri>meshes/Cosmic_Arena.STL</uri>
            </mesh>
          </geometry>
          <pose>0 0 0 0 0 0</pose>
        </collision>
        <visual name="visual">
          <geometry>
            <mesh>
              <uri>meshes/Cosmic_Arena.STL</uri> <!-- TODO: Change this visual to a .dae file with colors. -->
