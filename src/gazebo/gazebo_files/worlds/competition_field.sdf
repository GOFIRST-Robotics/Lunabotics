    <include>
      <uri>package://ros_gz_description/models/master_ASM</uri>
      <pose>3 1 1 0 0 0</pose>
    </include>

    <include>
      <uri>https://fuel.gazebosim.org/1.0/OpenRobotics/models/Falling%20Rock%204</uri>
      <pose>0 2 1 0 0 0</pose>
