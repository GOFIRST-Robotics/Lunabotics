    </include>

    <include>
      <uri>https://fuel.gazebosim.org/1.0/OpenRobotics/models/Falling%20Rock%204</uri>
      <pose>2 1.25 1 0 0 0</pose>
      <name>Rock 3</name>
    </include>

    <include>
